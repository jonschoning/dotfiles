*utf8:                  1
*saveLines:             999999

! Xft.dpi:        70
Xft.antialias:  true
Xft.rgba:       rgb
Xft.hinting:    true
Xft.hintstyle:  hintfull

! -- Fonts -- urxvt.font:xft:Bitstream Vera Sans Mono:size=9 !
! -- Fonts -- urxvt.boldfont:xft:Bitstream Vera Sans Mono Bold:size=9 !
urxvt.font:xft:Iosevka:size=10
urxvt.boldfont:xft:Iosevka Bold:size=10

! -- urxvt.font:xft:Bitstream Vera Sans Mono:size=9 !
! -- urxvt.boldfont:xft:Bitstream Vera Sans Mono Bold:size=9 !
urxvt.cursorColor:#ff0099
urxvt*iso14755: false

urxvt*depth: 32 
urxvt*background: [90]#000000
! urxvt*inheritPixmap: true
! urxvt*.transparent: true
! URxvt*.shading: 0 to 99 darkens, 101 to 200 lightens
! urxvt*.shading: 50

URxvt.perl-ext-common:      default,matcher,font-size,selection-to-clipboard
! URxvt.url-launcher:         /usr/bin/sensible-browser
URxvt.matcher.button:       1
URxvt.url-select.underline: true

URxvt.font-size.step: 1
URxvt.keysym.C-Up:   perl:font-size:incglobal
URxvt.keysym.C-Down:   perl:font-size:decglobal


!! get option over to work
URxvt.keysym.M-Left:        \033[1;5D
URxvt.keysym.M-Right:       \033[1;5C
URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

